<?xml version="1.0" encoding="UTF-8"?>
<web-app>
	<display-name>HelloWorld</display-name>

	<welcome-file-list>
		<!-- <welcome-file>loop.jsp</welcome-file> -->
		<!-- <welcome-file>index.html</welcome-file> -->
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>JavaWebStudy</servlet-name>
		<servlet-class>org.nhnnext.JavaWebStudy</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>JavaWebStudy</servlet-name>
		<url-pattern>/hello/</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Login</servlet-name>
		<!-- <servlet-class>Login.jsp</servlet-class> -->
 		<servlet-class>org.nhnnext.LoginServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login/</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Clock</servlet-name>
		<servlet-class>org.nhnnext.ClockServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Clock</servlet-name>
		<url-pattern>/clock/</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>BBS</servlet-name>
		<servlet-class>org.nhnnext.BbsServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>BBS</servlet-name>
		<url-pattern>/bbs/</url-pattern>
	</servlet-mapping>


</web-app>


